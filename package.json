{
  "name": "@voxpelli/forms-utils",
  "version": "2.1.1",
  "license": "MIT",
  "description": "A set of useful utilities for the forms module",
  "author": "Pelle Wessman <pelle@kodfabrik.se>",
  "homepage": "https://github.com/voxpelli/node-forms-utils",
  "repository": {
    "type": "git",
    "url": "git://github.com/voxpelli/node-forms-utils"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "sideEffects": false,
  "main": "index.js",
  "types": "index.d.ts",
  "browser": "browser/index.js",
  "files": [
    "index.js",
    "index.d.ts",
    "index.d.ts.map",
    "browser/*.js",
    "browser/*.d.ts",
    "browser/*.d.ts.map",
    "lib/*.js",
    "lib/*.d.ts",
    "lib/*.d.ts.map"
  ],
  "scripts": {
    "build:0": "run-s clean",
    "build:1-declaration": "tsc -p declaration.tsconfig.json",
    "build:2-add-ignores": "ts-ignore-import '**/*.d.ts'",
    "build": "run-s build:*",
    "check:dependency-check": "dependency-check *.js 'browser/**/*.js' 'test/**/*.js' --no-dev  --detective precinct -i @types/*",
    "check:installed-check": "installed-check -i eslint",
    "check:eslint": "eslint .",
    "check:typescript": "tsc",
    "check": "run-s clean && run-p check:*",
    "clean:declarations": "rm -rf $(find . -maxdepth 2 -type f -name '*.d.ts')",
    "clean": "run-p clean:*",
    "prepublishOnly": "run-s build",
    "test": "run-s check"
  },
  "devDependencies": {
    "@hdsydsvenskan/ts-ignore-import": "^2.0.0",
    "@types/forms": "^1.3.0",
    "@types/node": "^12.0.0",
    "@voxpelli/eslint-config": "10.0.0",
    "@voxpelli/eslint-config-jsdoc-ts": "^0.3.1",
    "dependency-check": "^4.1.0",
    "eslint": "^7.16.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-compat": "^3.9.0",
    "eslint-plugin-es": "^4.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.9",
    "eslint-plugin-mocha": "^8.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-unicorn": "^24.0.0",
    "forms": "^1.3.2",
    "installed-check": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "precinct": "^6.3.1",
    "typescript": "^3.9.7"
  },
  "peerDependencies": {
    "forms": "^1.2.0"
  },
  "dependencies": {
    "@hdsydsvenskan/dom-utils": "^1.0.0"
  }
}
